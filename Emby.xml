<?xml version="1.0" encoding="utf-8"?>
<Container>
    <Beta>False</Beta>
    <Name>emby</Name>
    <Repository>emby/embyserver:latest</Repository>
    <Registry>https://registry.hub.docker.com/u/emby/embyserver/</Registry>
    <Network>host</Network>
    <Shell>sh</Shell>
    <Privileged>false</Privileged>
    <Category>MediaServer:Video MediaServer:Music MediaServer:Photos</Category>
    <WebUI>http://[IP]:[PORT:8096]/</WebUI>
    <Icon>https://raw.githubusercontent.com/MediaBrowser/Emby.Resources/master/images/Logos/logoicon.png</Icon>
    <Networking>
        <Mode>host</Mode>
        <Publish>
            <Port>
                <HostPort>8096</HostPort>
                <ContainerPort>8096</ContainerPort>
                <Protocol>tcp</Protocol>
            </Port>
        </Publish>
    </Networking>
    <Data>
        <Volume>
            <HostDir>/mnt/user/appdata/emby</HostDir>
            <ContainerDir>/config</ContainerDir>
            <Mode>rw</Mode>
        </Volume>
        <Volume>
            <HostDir>media</HostDir>
            <ContainerDir>/mnt/media</ContainerDir>
            <Mode>ro</Mode>
        </Volume>
        <Volume>
            <HostDir>nsfw</HostDir>
            <ContainerDir>/mnt/nsfw</ContainerDir>
            <Mode>ro</Mode>
        </Volume>
    </Data>
    <Environment>
        <Variable>
            <Name>UID</Name>
            <Value>2</Value>
            <Mode/>
        </Variable>
        <Variable>
            <Name>GID</Name>
            <Value>2</Value>
            <Mode/>
        </Variable>
        <Variable>
            <Name>GIDLIST</Name>
            <Value>2</Value>
            <Mode/>
        </Variable>
    </Environment>
</Container>