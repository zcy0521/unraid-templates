<?xml version="1.0" encoding="utf-8"?>
<Container>
    <Beta>False</Beta>
    <Name>mysql</Name>
    <Repository>mysql:latest</Repository>
    <Registry>https://hub.docker.com/_/mysql</Registry>
    <Network>bridge</Network>
    <Shell>sh</Shell>
    <Privileged>false</Privileged>
    <Networking>
        <Mode>bridge</Mode>
        <Publish>
            <Port>
                <HostPort>3306</HostPort>
                <ContainerPort>3306</ContainerPort>
                <Protocol>tcp</Protocol>
            </Port>
        </Publish>
    </Networking>
    <Data>
        <Volume>
            <HostDir>/mnt/user/appdata/mysql/conf</HostDir>
            <ContainerDir>/etc/mysql/conf.d</ContainerDir>
            <Mode>ro</Mode>
        </Volume>
        <Volume>
            <HostDir>/mnt/user/appdata/mysql/datadir</HostDir>
            <ContainerDir>/var/lib/mysql</ContainerDir>
            <Mode>rw</Mode>
        </Volume>
    </Data>
    <Environment>
        <Variable>
            <Name>MYSQL_ROOT_PASSWORD</Name>
            <Value>123456</Value>
            <Mode/>
        </Variable>
        <Variable>
            <Name>MYSQL_ROOT_HOST</Name>
            <Value>%</Value>
            <Mode/>
        </Variable>
    </Environment>
</Container>